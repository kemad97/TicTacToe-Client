                  <String fx:value="numbers" />
                  <String fx:value="label-with-shadow" />
               </styleClass>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
            <Label fx:id="win_matches" contentDisplay="CENTER" layoutX="304.0" layoutY="306.0" prefHeight="40.0" prefWidth="40.0" text="1" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
