            <Circle fill="DODGERBLUE" layoutX="97.0" layoutY="84.0" radius="59.0" stroke="BLACK" strokeType="INSIDE" styleClass="label-with-shadow" />
            <ImageView fitHeight="83.0" fitWidth="118.0" layoutX="38.0" layoutY="43.0" pickOnBounds="true" preserveRatio="true">
