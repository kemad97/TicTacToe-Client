<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.client.RecScreen.RecScreenController">
   <children>
      <ImageView fx:id="logo" fitHeight="69.0" fitWidth="94.0" layoutX="24.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../media/images/logo.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="430.0" fitWidth="600.0" layoutY="-1.0">
         <image>
            <Image url="@../../../media/images/background.png" />
         </image>
      </ImageView>
      <BorderPane layoutX="80.0" layoutY="93.0" prefHeight="297.0" prefWidth="441.0">
         <top>
            <TitledPane animated="false" prefHeight="299.0" prefWidth="441.0" text="Recent Games" BorderPane.alignment="CENTER">
               <content>
                  <ListView fx:id="fileListView" onMouseClicked="#openGameLogFile" prefHeight="0.0" prefWidth="441.0" />
               </content></TitledPane>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
